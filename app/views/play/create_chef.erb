<h1> CREATE YOUR CHEF <h2>

<%= form_for(@chef, url: {action: 'chef_created'}, method: :post) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.submit %>
    <% @arr.each do |img_name| %>
        <%= check_box("chef_img", "chef_key", {:multiple => false}, img_name, nil) %>
        <%=image_tag(img_name)%>
    <% end %>
<% end %>




