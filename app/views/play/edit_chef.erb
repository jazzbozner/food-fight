<h1> EDIT A CHEF <h2>

<%= form_for(@chef, url: {action: "chef_editted"}, method: :post) do |f| %>
    <%= f.collection_select :id, Chef.all, :id, :name %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <% @arr.each do |img_name| %>
        <%= radio_button("chef", "image_name", img_name) %>
        <%= image_tag(img_name)%>
        <br>
    <% end %>
    <%= f.submit "Edit Chef!" %>
<% end %>